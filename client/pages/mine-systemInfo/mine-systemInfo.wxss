.page{
    width: 100%;
    height: 100%;
}
.action{
    width: 200rpx!important;
}
.cu-list>.cu-item .move{
    width: 130rpx;
}
.cu-list>.cu-item.move-cur {
    transform:translateX(-130rpx);
}
.cu-list.menu-avatar>.cu-item{
    background-color: var(--black);
    border-radius: 10rpx;
    margin: 30rpx 0;
}
.cu-list.menu-avatar>.cu-item{
    height: 200rpx!important;
}
.forNotice{
    width: 100%;
    height: 100%;
    display: flex;
    align-items: flex-start;
    justify-content: center;
}
.forNotice image{
    width: 400rpx;
    height: 400rpx;
    margin: 40rpx 0;
    border-radius: 10rpx;
}
.notice-form{
    font-size: 30rpx;
    font-weight: bold;
    margin: 30rpx 0;
}
.QR_code{
    width: 200rpx;
    height: 200rpx;
}
